{
    "manifest_version": 3,
    "name": "Duplicate Tab Counter",
    "version": "1.0",
    "description": "Counts and optionally closes duplicate tabs.",
    "permissions": [
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {  
      "service_worker": "background.js"
    }
  }